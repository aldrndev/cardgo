{
  "expo": {
    "name": "Card Go",
    "slug": "card-go",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/new-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/new-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00A896"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aldrnmrsd.cardgo",
      "buildNumber": "1.2.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/new-icon.png",
        "backgroundColor": "#00A896"
      },
      "package": "com.aldrnmrsd.cardgo",
      "versionCode": 5,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/new-icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/new-icon.png",
          "color": "#00A896"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b0a34643-a9ac-4c13-a350-a45ebb87d85b"
      }
    }
  }
}
